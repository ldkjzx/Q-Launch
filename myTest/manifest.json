{

    "manifest_version": 2,
    "name": "Q-Launch",
    "description": "Let's Roll n'Rock!!!",
    "omnibox": { "keyword" : "q-launch" }, 
    "icons":{
        "16": "build/img/icon16.png",
        "48": "build/img/icon48.png",
        "128": "build/img/icon128.png"
    },
    "version": "0.1",
    
    "permissions": ["<all_urls>",
    	"tabs",
    	"activeTab",
    	"notifications",
    	"storage"
    ],
    
	"browser_action": {
        "default_icon": {
            "19": "build/img/icon19_G.png",
            "38": "build/img/icon38_G.png"
        },
        "default_title": "Q-Launch",
        "default_popup": "popup.html"
    },

    "background": {
    	"page": "background.html"
    },

    "chrome_url_overrides": {
    	"newtab": "newtab.html"
    }

}
