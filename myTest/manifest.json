{

    "manifest_version": 2,
    "name": "myTest",
    "description": "It's myTest!!!",
    "version": "0.1",
    
    "permissions": ["<all_urls>",
    	"tabs",
    	"activeTab",
    	"notifications",
    	"storage"
    ],
    
	"browser_action": {
        "default_icon": {
            "19": "images/icon19_G.png",
            "38": "images/icon38_G.png"
        },
        "default_title": "stock helper",
        "default_popup": "popup.html"
    },

    "background": {
    	"page": "background.html",
    	"scripts": ["background.js"]	
    },

    "chrome_url_overrides": {
    	"newtab": "newtab.html"
    }

}
